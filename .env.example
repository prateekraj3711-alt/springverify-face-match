# Copy this file to .env and fill in your actual values

# Port for the server (optional, defaults to 3000)
PORT=3000

# IDfy API Credentials for Face Matching
# Get your credentials from IDfy dashboard
IDFY_API_KEY=your_idfy_api_key_here
IDFY_ACCOUNT_ID=your_idfy_account_id_here

